  "source": "pragma solidity ^0.4.18;\nimport \"../contracts/CustomOwnables.sol\";\n\ncontract LocalGvt is CustomOwnables {\n\n    //local government adds, removes, updates refugee info\n    //registered refugees are stored in a database with an ID mapping to refugee details\n    //refugee details are stored in a struct\n\n    //map refugees id/contract id to Refugee struct\n    mapping(uint => Refugee) refugees;\n\n    struct Refugee {\n        uint id;\n        uint tokenBalance;\n        string dob;\n        string name;\n        string citizenship; \n        //etc\n    }\n\n    function registerRefugee () external view onlyLocalGvt {\n        //adds to refugees mapping\n    }\n\n    function deregisterRefugee () external view  onlyLocalGvt {\n        //deletes from refugees\n    }\n\n    function updateRefugee () external view onlyLocalGvt {\n        //updates specified refugee\n    }\n\n}",
